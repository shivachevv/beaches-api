{
  "name": "beaches_api",
  "version": "1.0.0",
  "description": "rest api for the app Beaches",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --config nodemon.json ./src/index.ts",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo-all": "npx sequelize-cli db:migrate:undo:all",
    "gen-migration": "npx sequelize migration:generate --name",
    "seed-all": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo-all": "npx sequelize-cli db:seed:undo:all",
    "gen-seed": "npx sequelize seed:generate --name"
  },
  "author": "Nikola Shivachev",
  "license": "ISC",
  "dependencies": {
    "@types/sequelize": "^4.28.13",
    "babel-cli": "^6.26.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.15",
    "pg": "8.1.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-cli": "^6.4.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.23",
    "@types/uuid": "^8.3.4",
    "cors": "^2.8.5",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}